#!/bin/bash
#SBATCH -J TBSLAS_CONV
#SBATCH --export=ALL
#SBATCH -n 16              # total number of mpi tasks requested
#SBATCH -N 16              # nodes
#SBATCH -p normal     # queue (partition) -- normal, development, etc.
#SBATCH -t 02:00:00        # run time (hh:mm:ss) - 1.5 hours

nodes = 16
cores = 16

WORK_DIR=${PWD}
cd ${WORK_DIR}

module load fftw3
module load python

export TBSLAS_RESULT_DIR=${WORK}/conv
mkdir -p ${TBSLAS_RESULT_DIR};

python ./conv.py nodes cores
### End of script
